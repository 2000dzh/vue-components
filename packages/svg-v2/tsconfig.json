{
	"compilerOptions": {
		"target": "ES2018",
		"jsx": "preserve",
		"lib": ["ES2018", "DOM"],
		"baseUrl": ".",
		"module": "ESNext",
		"moduleResolution": "node",
		"types": [],
		"strict": true,
		"noImplicitAny": false,
		"declaration": true,
		"declarationDir": "./dist/dl-svg-v2/types",
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"references": [{ "path": "./tsconfig.build.json" }],
	"include": ["./src/*", "./components/**/*"]
}
